{
  "name": "woo1.kr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cp contrib/piping.js node_modules/piping/lib/piping.js && npm-run-all --parallel dev:*",
    "dev:server": "NODE_ENV=development node bin/server",
    "dev:packager": "NODE_ENV=development node bin/packager",
    "build": "NODE_ENV=production webpack --verbose --colors --display-error-details --config ./tools/webpack.production.js",
    "start": "NODE_ENV=production node bin/server",
    "test:node": "NODE_ENV=test mocha $(find src/server -name '*-test.js') --compilers js:./tools/babel-require",
    "db:up": "sequelize db:migrate",
    "db:down": "sequelize db:migrate:undo",
    "db:down:all": "sequelize db:migrate:undo:all",
    "db:create": "sequelize migration:create",
    "db:status": "sequelize db:migrate:status",
    "db:seed:create": "sequelize seed:create",
    "db:seed:up": "sequelize db:seed:all",
    "db:seed:down": "sequelize db:seed:undo:all"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "es2015": true
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-flow-strip-types"
    ]
  },
  "dependencies": {
    "angular": "^1.6.3",
    "angular-sanitize": "^1.6.3",
    "angular-toastr": "^2.1.1",
    "angular-ui-router": "1.0.0-rc.1",
    "assets-webpack-plugin": "^3.5.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "bcrypt-nodejs": "^0.0.3",
    "boom": "^4.2.0",
    "bootstrap": "3.1.1",
    "catbox-redis": "^2.0.2",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.2",
    "dropzone": "^4.3.0",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "hapi": "^16.1.0",
    "hapi-es7-async-handler": "^1.0.6",
    "hapi-swagger": "^7.7.0",
    "highcharts": "^5.0.9",
    "highcharts-ng": "^1.0.1",
    "html-loader": "^0.4.5",
    "inert": "^4.1.0",
    "joi": "^10.2.2",
    "jquery": "^3.1.1",
    "karma": "^1.5.0",
    "kue": "^0.11.5",
    "kue-scheduler": "^0.7.1",
    "lodash.kebabcase": "^4.1.1",
    "mocha": "^3.2.0",
    "nes": "^6.4.2",
